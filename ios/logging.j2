logging snmp-authfail
logging trap informational
logging buffered 128000
logging reload warnings
no logging console
logging history size 10
logging source-interface {{ primary_ip4["interface"]["name"] }}
{% for server, attrib in config_context["logging"]["servers"].items() -%}
 logging host {{ server }} 
 {% if attrib["transport"] is defined %}
 logging host {{ server }} transport {{ attrib["transport"] }} 
 {% elif attrib["transport"] and attrib["port"] is defined %} 
 logging host {{ server }} transport {{ attrib["transport"] }} port {{ attrib["port"] }} 
 {% endif %}
{% endfor -%}

!
 
